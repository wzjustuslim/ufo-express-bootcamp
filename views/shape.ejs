<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Sighting Shapes</title>
</head>
<body>
  <h2>Sighting Shapes</h2>
  <div>
    <h3>Shape List</h3>
    <ul>
      <% shapes.forEach(element => { %>
        <li><a href="/shapes/<%= element.toLowerCase() %>"><%= element %></a></li>
      <% }); %>
    </ul>
  </div>
</body>
</html>